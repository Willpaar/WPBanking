{
    "builds": [
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
    {
      "src": "/",
      "dest": "/HTML/index.html"
    },
    {
      "src": "/about",
      "dest": "/HTML/about.html"
    },
    {
      "src": "/whywp",
      "dest": "/HTML/whywp.html"
    },
    {
      "src": "/contact",
      "dest": "/HTML/contact.html"
    },
    {
      "src": "/login",
      "dest": "/HTML/login.html"
    },
    
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    
    {
        "src": "/styles/(.*)",
        "dest": "/public/styles/$1"
      },
  
     
      {
        "src": "/images/(.*)",
        "dest": "/public/images/$1"
      },
  

      {
        "src": "/scripts/(.*)",
        "dest": "/public/scripts/$1"
      }

    ]
  }
  